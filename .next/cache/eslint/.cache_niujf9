.select();\n    document.execCommand(\"copy\");\n    document.body.removeChild(textArea);\n    setCheckCopy(true);\n    console.log(\"URL copied to clipboard!\");\n  }\n\n  return (\n    <Layout title=\"Tracking - Status\">\n      <Container\n        id=\"logistics_tracking\"\n        style={{\n          margin: \"0px\",\n        }}\n        className=\"tracking\"\n      >\n        <div>\n          <h1 style={{}} className=\"head\">\n            Order Status\n          </h1>\n\n          <div className=\"head_body\">\n            <div className=\"d1\">\n              {typeof data?.files === \"string\" ? (\n                <img\n                  className=\"d-block\"\n                  // src={`data:image/png;base64,${JSON.parse(data?.files)[0]}`}\n                  src={JSON.parse(data?.files)[0]}\n                  alt=\"Kupon\"\n                />\n              ) : null}\n              <p>{data?.packageName}</p>\n            </div>\n\n            <div className=\"d2\">\n              <h3>Delivery Address</h3>\n              <p>{data?.deliveryAddress}</p>\n            </div>\n\n            <div className=\"d3\">\n              <p>\n                Tracking Code : <b>{data?.packageCode}</b>\n              </p>\n\n              {checkcopy ? (\n                <p className=\"p3\">Copied</p>\n              ) : (\n                <p\n                  onClick={() => handleCopy()}\n                  className=\"p3\"\n                  style={{\n                    cursor: \"pointer\",\n                  }}\n                >\n                  Copy Tracking Link{\" \"}\n                  <img\n                    width=\"20\"\n                    height=\"20\"\n                    src=\"https://img.icons8.com/ios-glyphs/30/copy.png\"\n                    alt=\"copy\"\n                  />\n                </p>\n              )}\n            </div>\n          </div>\n\n          <div className=\"status\">\n            <p\n              className=\"p1\"\n              style={{\n                background:\n                  data?.status == \"pending\"\n                    ? \"#e60000\"\n                    : data?.status == \"in transit\"\n                    ? \"#FD901A\"\n                    : \"green\",\n              }}\n            >\n              {\" \"}\n              <img\n                width=\"20\"\n                height=\"20\"\n                src={\n                  data?.status == \"pending\"\n                    ? \"https://img.icons8.com/ios-filled/50/clock--v1.png\"\n                    : data?.status == \"in transit\"\n                    ? \"https://img.icons8.com/ios/50/airplane-take-off.png\"\n                    : \"https://img.icons8.com/ios/50/airplane-landing.png\"\n                }\n                alt=\"clock--v1\"\n              />{\" \"}\n              <b>{data?.status}</b>\n            </p>\n            <p className=\"p2\">\n              {data?.status == \"pending\"\n                ? null\n                : data?.status == \"in transit\"\n                ? `your item was picked up @${new Date(\n                    data?.updated_at\n                  ).toLocaleString()}`\n                : `your item was delivered @${new Date(\n                    data?.updated_at\n                  ).toLocaleString()}`}\n            </p>\n          </div>\n        </div>\n      </Container>\n    </Layout>\n  );\n}\n","/home/mgenius/Desktop/WEB PROGRAMMING/kupon-main/pages/market/index.js",["344","345","346","347","348"],[],"import Layout from \"../../components/Layout\";\nimport Link from \"next/link\";\nimport $ from \"jquery\";\nimport React, { Fragment, useEffect, useState } from \"react\";\n// import 'slick-carousel/slick/slick';\nimport \"slick-carousel/slick/slick.css\";\nimport \"slick-carousel/slick/slick-theme.css\";\nimport Slider from \"react-slick\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { fetchData } from \"../../store/store\";\nimport Spinner from \"react-bootstrap/Spinner\";\n\nexport default function Home() {\n  const [shuffleData, setShuffleData] = useState();\n  // const [token, setToken] = useState(() => {\n  //   if (typeof window !== 'undefined') {\n  //     return localStorage.getItem('token');\n  //   }\n  // });\n  const dispatch = useDispatch();\n  let { data, isLoading } = useSelector((state) => state);\n\n  useEffect(() => {\n    if (typeof window !== \"undefined\") {\n      window?.addEventListener(\"scroll\", handleScroll); // add scroll event listener on component mount\n    }\n    return () => {\n      if (typeof window !== \"undefined\") {\n        window?.removeEventListener(\"scroll\", handleScroll); // remove scroll event listener on component unmount\n      }\n    };\n  }, []);\n\n  useEffect(() => {\n    dispatch(fetchData());\n  }, [dispatch]);\n\n  useEffect(() => {\n    if (data) {\n      // Shuffle the array using the Fisher-Yates algorithm\n      for (let i = data.length - 1; i > 0; i--) {\n        const j = Math.floor(Math.random() * (i + 1));\n        [data[i], data[j]] = [data[j], data[i]];\n      }\n\n      // Get the first six items using slice method\n      data = data.slice(0, 6);\n      setShuffleData(data);\n    }\n  }, [data]);\n\n  function handleScroll() {\n    if (typeof window !== \"undefined\") {\n      if (window?.innerWidth > 1199) {\n        const headerWrap = $(\".header-wrap\");\n        if (window?.pageYOffset > 145) {\n          headerWrap.addClass(\"stickyNav animated fadeInDown\");\n        } else {\n          headerWrap.removeClass(\"stickyNav fadeInDown\");\n        }\n      }\n    }\n  }\n\n  return (\n    <Fragment>\n      <Layout title=\"Home Page - Market\">\n        {/* {/*<!--Body Content-->*/}\n        <div id=\"page-content\">\n          {/* {/*<!--Home slider-->*/}\n          <div className=\"slideshow slideshow-wrapper pb-section sliderFull\">\n            <Slider className=\"home-slideshow\">\n              <div className=\"slide\">\n                <div className=\"b